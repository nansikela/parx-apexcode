    function deleteSpaces() {
        var content = $ic('#txtArea').val();
        var records = content.split("\n");
        var newText = '';
        var Ausdruck = /\S+/;
        
        for(var i = 0; i < records.length; i++) {
            var row = records[i];
            if(row == null || row.length == 0 || row == '' || !Ausdruck.test(row)){
                continue;
            }
            if(i >= records.length-1)
                newText += row; 
            else
                newText += row + '\n';
        }
        $ic('#txtArea').val(newText);
    } 
   
