    <validationRules>
        <fullName>Inactive_Promotion_Activity</fullName>
        <active>true</active>
        <description>Don&apos;t let them select an inactive promotion</description>
        <errorConditionFormula>Promotion_Activity__r.Active__c  = false</errorConditionFormula>
        <errorDisplayField>Promotion_Activity__c</errorDisplayField>
        <errorMessage>Please select an active promotion activity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_InternalOrder_Update_Validation</fullName>
        <active>true</active>
        <description>This validation rules does not allow the user to update the Internal Order if the Promotion Status is not Created</description>
        <errorConditionFormula>IF(AND(ISCHANGED(Internal_Order__c),NOT(ISNEW()), TEXT(Promotion_Status__c) != &quot;Created&quot;) , true,false)</errorConditionFormula>
        <errorDisplayField>Internal_Order__c</errorDisplayField>
        <errorMessage>You can only update the internal order if the promotion status is Created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_Mark_For_Deletion_Validation</fullName>
        <active>true</active>
        <description>NPA Rollout: Ensures that Promotions with Status not equal to Created cannot be Marked for Deletion</description>
        <errorConditionFormula>AND ( $RecordType.Name = &quot;National Promotion - US&quot;, NOT( ISPICKVAL( Promotion_Status__c, &apos;Created&apos;)),  Mark_for_Delete__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Mark_for_Delete__c</errorDisplayField>
        <errorMessage>Only Promotions with Status Created can be marked for deletion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_Start_Date_Validation</fullName>
        <active>true</active>
        <description>NPA Rollout: Validation rule to make sure Start Date is not greater than End Date</description>
        <errorConditionFormula>AND( ( $RecordType.Name =  &quot;National Promotion - US&quot; ),( Promotion_Start_Date__c &gt; Promotion_End_Date__c ))</errorConditionFormula>
        <errorDisplayField>Freeze_Date__c</errorDisplayField>
        <errorMessage>Start Date cannot be greater than End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_Status_Validation_Active</fullName>
        <active>true</active>
        <description>NPA Rollout: Validation rule to ensure Promotion Status cannot not be changed from Active to Created.</description>
        <errorConditionFormula>AND( 
	$RecordType.Name = &quot;National Promotion - US&quot;,
	ISCHANGED( Promotion_Status__c ), 
	TEXT(PRIORVALUE( Promotion_Status__c )) = &apos;Active&apos;,
	TEXT( Promotion_Status__c ) = &apos;Created&apos;
)</errorConditionFormula>
        <errorDisplayField>Promotion_Status__c</errorDisplayField>
        <errorMessage>Cannot change Promotion Status from Active to Created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_Status_Validation_Cancelled</fullName>
        <active>true</active>
        <description>NPA Rollout: Validation rule to ensure Promotion Status cannot not be changed from Cancelled to Created.</description>
        <errorConditionFormula>AND( 
	$RecordType.Name = &quot;National Promotion - US&quot;,
	ISCHANGED( Promotion_Status__c ), 
	TEXT(PRIORVALUE( Promotion_Status__c )) = &apos;Cancelled&apos;,
	TEXT( Promotion_Status__c ) = &apos;Created&apos;
)</errorConditionFormula>
        <errorDisplayField>Promotion_Status__c</errorDisplayField>
        <errorMessage>Cannot change Promotion Status from Cancelled to Created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NPA_Tertile_Validation</fullName>
        <active>true</active>
        <description>Rule to ensure that the Start Date and End Date are within the same Tertile.</description>
        <errorConditionFormula>AND(
	$RecordType.Name = &quot;National Promotion - US&quot;,
	OR(
		AND (MONTH(Promotion_Start_Date__c)  &lt;= 4, MONTH(Promotion_End_Date__c) &gt; 4),
		AND (MONTH(Promotion_Start_Date__c)  &lt;= 8, MONTH(Promotion_End_Date__c) &gt; 8),
		YEAR(Promotion_Start_Date__c) &lt;&gt; YEAR(Promotion_End_Date__c)
	)
)</errorConditionFormula>
        <errorDisplayField>Freeze_Date__c</errorDisplayField>
        <errorMessage>Start Date and End Date should be within a single Tertile</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotion_Win_In_Store</fullName>
        <active>true</active>
        <description>Win In Store Requires Draw Date</description>
        <errorConditionFormula>Sub_Type__c  = &apos;Win In Store Promotion&apos;  &amp;&amp; ISBLANK ( Draw_Date__c)</errorConditionFormula>
        <errorDisplayField>Draw_Date__c</errorDisplayField>
        <errorMessage>This is a Win In Store promotion.  A draw date must be entered.</errorMessage>
    </validationRules>
